            gameStats = {
                score: 0,
                bombHits: 0,
                iceHits: 0,
                flowersSkipped: 0,
                isGameOver: false,
            };
        }

        function getRandomDelay() {
            return Math.random() * (GAME_SETTINGS.maxDelayMs - GAME_SETTINGS.minDelayMs) + GAME_SETTINGS.minDelayMs;
        }

